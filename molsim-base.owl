    <!-- http://purl.obolibrary.org/obo/MOLSIM_000723 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000723">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000692"/>
        <obo:IAO_0000115 xml:lang="en">Protein Data Bank - The single worldwide archive for information about the 3D structures of large biological molecules, including proteins, nucleic acids, and complex assemblies, determined experimentally (primarily by X-ray crystallography, NMR, and cryo-EM). PDB is a fundamental resource for structural biology, biochemistry, and drug discovery. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">PDB</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000724 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000724">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000692"/>
        <obo:IAO_0000115 xml:lang="en">One of the founding members of the Worldwide Protein Data Bank (wwPDB) organization, responsible for managing and disseminating PDB data. PDBe provides access to the global archive of macromolecular structures and offers additional curated data, analysis tools, and resources for structural biology. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">PDBe</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000725 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000725">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000692"/>
        <obo:IAO_0000115 xml:lang="en">A founding member of the Worldwide Protein Data Bank (wwPDB) organization, serving as the Asian regional center for the PDB. PDBj collects, processes, and disseminates macromolecular structure data, and develops tools and resources for structural biology research in Japan and globally. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">PDBj</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000726 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000726">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000692"/>
        <obo:IAO_0000115 xml:lang="en">A pictorial database that provides an at-a-glance overview of the contents of each 3D macromolecular structure deposited in the Protein Data Bank (PDB). PDBsum presents schematic diagrams of molecular interactions, protein domains, ligand binding sites, and other structural features. It is a useful tool for quickly understanding PDB entries. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">PDBsum</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000727 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000727">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000693"/>
        <obo:IAO_0000115 xml:lang="en">Nucleic Acid Database - A relational database that archives and disseminates three-dimensional structural information about nucleic acids (DNA, RNA) and their complexes, determined experimentally by X-ray crystallography, NMR, or electron microscopy. NDB provides tools for searching and analyzing these structures. It is the primary repository for nucleic acid 3D structures. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">NDB</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000728 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000728">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000694"/>
        <obo:IAO_0000115 xml:lang="en">Electron Microscopy Data Bank - The primary public archive for three-dimensional electron microscopy density maps of biomolecular complexes and subcellular structures. EMDB stores 3D reconstructions obtained from techniques like cryo-electron microscopy (cryo-EM), electron tomography, and single particle analysis. It is essential for structural biologists studying large macromolecular assemblies. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">EMDB</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000729 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000729">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000694"/>
        <obo:IAO_0000115 xml:lang="en">Electron Microscopy Public Image Archive - A public resource for raw 2D electron microscopy image data, including movies and tilt series, that underpins 3D structures in EMDB or other studies. EMPIAR promotes transparency, reproducibility, and the development of new image processing methods by providing access to the primary experimental data. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">EMPIAR</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000730 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000730">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000697"/>
        <obo:IAO_0000115 xml:lang="en">The NIH genetic sequence database, an annotated collection of all publicly available DNA sequences, maintained by the National Center for Biotechnology Information (NCBI). As part of the INSDC, GenBank is a foundational resource for molecular biology, genomics, and evolutionary studies worldwide. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">GenBank</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000731 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000731">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000708"/>
        <obo:IAO_0000115 xml:lang="en">UniProt Knowledgebase - A comprehensive, high-quality, and freely accessible resource of protein sequence and functional information, created by joining the Swiss-Prot, TrEMBL, and PIR-PSD databases. UniProtKB consists of two sections: Swiss-Prot (manually annotated and reviewed) and TrEMBL (computationally annotated). It is a central hub for protein information. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">UniProtKB</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000732 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000732">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000697"/>
        <obo:IAO_0000115 xml:lang="en">European Nucleotide Archive - A comprehensive public repository for nucleotide sequencing information, covering raw reads, assembled sequences, and functional annotations, from all domains of life. As part of the INSDC, ENA collaborates with GenBank and DDBJ to provide a global archive for sequence data. It is a fundamental resource for genomics and bioinformatics. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">ENA</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000733 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000733">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000697"/>
        <obo:IAO_0000115 xml:lang="en">DNA Data Bank of Japan - one of the three major international nucleotide sequence databases, along with GenBank (NCBI) and ENA (EMBL-EBI), forming the International Nucleotide Sequence Database Collaboration (INSDC). DDBJ collects, annotates, and provides public access to DNA and RNA sequences from all organisms. It is a fundamental resource for genomics and molecular biology. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">DDBJ</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000734 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000734">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000696"/>
        <obo:IAO_0000115 xml:lang="en">The manually annotated and reviewed section of the UniProt Knowledgebase (UniProtKB). Swiss-Prot entries contain high-quality protein sequence information, detailed functional annotations, and extensive cross-references, derived from literature and curator evaluation. It represents the gold standard for protein annotation. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">UniProtKB/Swiss-Prot</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000811 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000811">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000696"/>
        <obo:IAO_0000115 xml:lang="en">The computationally annotated section of the UniProt Knowledgebase (UniProtKB), containing protein sequences that are not yet manually curated. TrEMBL entries are largely derived from the translation of coding sequences in nucleotide sequence databases and are automatically annotated. It provides broad sequence coverage. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">UniProtKB/TrEMBL</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000919 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000919">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000696"/>
        <obo:IAO_0000115 xml:lang="en">The NCBI Reference Sequence (RefSeq) collection for protein data, offering a curated, non-redundant set of protein sequences, often linked to corresponding nucleotide sequences and genomic annotations. RefSeq proteins serve as stable reference points for protein identification, annotation, and comparative analysis. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">NCBI RefSeq Protein</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000920 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000920">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000696"/>
        <obo:IAO_0000115 xml:lang="en">Protein Information Resource - A long-standing public bioinformatics resource that created the Protein Sequence Database (PSD), one of the first curated collections of protein sequences. PIR now contributes to UniProt and develops other resources like iProClass for protein classification and functional annotation. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">PIR</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000922 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000922">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000697"/>
        <obo:IAO_0000115 xml:lang="en">The NCBI Reference Sequence (RefSeq) collection for nucleotide data, providing a comprehensive, integrated, non-redundant, and well-annotated set of sequences for chromosomes, genomic regions, transcripts, and RNAs. RefSeq aims to provide stable reference sequences for various organisms. It is a critical resource for genomics and gene annotation</obo:IAO_0000115>
        <rdfs:label xml:lang="en">NCBI RefSeq Nucleotide</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000929 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000929">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000699"/>
        <obo:IAO_0000115 xml:lang="en">A public repository within PubChem that contains validated chemical structure information for small molecules, including their identifiers, properties, and links to related information. PubChem Compound is a comprehensive resource for chemical information and a key component of the PubChem suite. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">PubChem Compound</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000930 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000930">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000699"/>
        <obo:IAO_0000115 xml:lang="en">Chemical Entities of Biological Interest - A freely available dictionary and ontology of molecular entities focused on &apos;small&apos; chemical compounds that are either products or reactants in biological processes. ChEBI provides nomenclature, structural information, and links to other databases for a wide range of biologically relevant chemicals. It is essential for cheminformatics and systems biology. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">ChEBI</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000931 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000931">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000699"/>
        <obo:IAO_0000115 xml:lang="en">A free database of commercially available chemical compounds prepared in ready-to-dock, 3D formats for virtual screening and computational drug discovery. ZINC20 contains millions of purchasable compounds with annotated properties, facilitating large-scale ligand docking and cheminformatics studies. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">ZINC20</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000932 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000932">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000700"/>
        <obo:IAO_0000115 xml:lang="en">A comprehensive, freely accessible online database containing detailed information on drugs, drug targets, drug interactions, and drug metabolism. It combines detailed drug data with comprehensive drug target information, making it a vital resource for pharmacology, medicinal chemistry, and pharmaceutical research. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">DrugBank</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000933 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000933">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000699"/>
        <obo:IAO_0000115 xml:lang="en">A free chemical structure database providing fast access to over millions of structures, properties, and associated information from various sources. It allows users to search by chemical name, synonym, trade name, registry number, or structure. ChemSpider is a comprehensive resource for chemists and researchers needing chemical information. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">ChemSpider</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000934 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000934">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000700"/>
        <obo:IAO_0000115 xml:lang="en">Therapeutic Target Database - A database providing comprehensive information about therapeutic protein and nucleic acid targets, the approved and investigational drugs directed at these targets, and the corresponding diseases. TTD links target information with drug development pipelines and clinical trial data. It is a useful resource for drug discovery and pharmacology. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">TTD</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000935 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000935">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000700"/>
        <obo:IAO_0000115 xml:lang="en">A website operated by the U.S. National Library of Medicine (NLM) that provides high-quality information about marketed drugs, including FDA-approved drug labels (package inserts). It offers comprehensive information for healthcare providers and the public on drug ingredients, indications, dosage, warnings, and adverse reactions. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">DailyMed</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000957 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000957">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000707"/>
        <obo:IAO_0000115 xml:lang="en">A manually curated, large-scale database of bioactive drug-like small molecules and their quantitative bioactivity data against drug targets (primarily proteins). It brings together chemical, bioactivity, and genomic data to aid the translation of genomic information into effective new drugs. ChEMBL is a critical resource for drug discovery, pharmacology, and medicinal chemistry.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">ChEMBL</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000958 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000958">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000701"/>
        <obo:IAO_0000115 xml:lang="en">Mass Spectrometry Interactive Virtual Environment - A public data repository for mass spectrometry (MS) datasets, particularly from proteomics experiments, developed by the Center for Computational Mass Spectrometry at UCSD. MassIVE facilitates the deposition, storage, sharing, and reanalysis of MS data, supporting open science in the proteomics community. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">MassIVE</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000959 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000959">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000701"/>
        <obo:IAO_0000115 xml:lang="en">An integrated spectral database system for organic compounds, maintained by the National Institute of Advanced Industrial Science and Technology (AIST), Japan. SDBS contains NMR (1H, 13C), mass spectra, IR, Raman, and ESR spectra for a large number of organic compounds. It is a valuable resource for chemical identification and structural elucidation. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">SDBS</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000960 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000960">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000702"/>
        <obo:IAO_0000115 xml:lang="en">Biological Magnetic Resonance Data Bank - A repository that collects, archives, and disseminates Nuclear Magnetic Resonance (NMR) spectroscopic data from biological macromolecules and metabolites. This includes assigned chemical shifts, coupling constants, relaxation rates, and derived structural information like distance and dihedral angle restraints. BMRB is the primary worldwide archive for biomolecular NMR data. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">BMRB</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000961 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000961">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000702"/>
        <obo:IAO_0000115 xml:lang="en">Complex Carbohydrate Magnetic Resonance Database for NMR - A specialized NMR database focusing on the assigned chemical shifts of complex carbohydrates (oligosaccharides and polysaccharides) and their complexes with proteins. It serves as a resource for researchers studying carbohydrate structures and interactions using NMR spectroscopy. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">ComplexCARP</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000962 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000962">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000703"/>
        <obo:IAO_0000115 xml:lang="en">A database that integrates predictive signatures for protein families, domains, and functional sites from multiple member databases (e.g., Pfam, PROSITE, SMART, CDD, CATH, SCOP). InterPro provides a comprehensive overview of protein sequence features, aiding in functional annotation and comparative analysis. It is a powerful tool for protein characterization. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">InterPro</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000964 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000964">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000704"/>
        <obo:IAO_0000115 xml:lang="en">A large collection of protein families, each represented by multiple sequence alignments and hidden Markov models (HMMs). Pfam is used to identify conserved domains and families in protein sequences, providing insights into their function and evolutionary relationships. It is a widely used resource for protein sequence annotation. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">Pfam</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000965 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000965">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000704"/>
        <obo:IAO_0000115 xml:lang="en">A web-based resource used for the identification and annotation of genetically mobile domains and the analysis of domain architectures in proteins. SMART contains manually curated models for hundreds of domains involved in signaling, extracellular functions, and chromatin regulation. It is a valuable tool for protein sequence analysis and functional prediction. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">SMART</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000966 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000966">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000704"/>
        <obo:IAO_0000115 xml:lang="en">A database of protein domains, families, and functional sites, characterized by patterns (regular expressions) or profiles (position-specific scoring matrices) derived from multiple sequence alignments. PROSITE is used to identify known motifs and domains in protein sequences, aiding in functional prediction. It is part of the SIB ExPASy server. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">PROSITE</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000967 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000967">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000704"/>
        <obo:IAO_0000115 xml:lang="en">Conserved Domain Database by NCBI -  A database maintained by NCBI containing manually curated models of ancient domains and full-length proteins, as well as models imported from other sources like Pfam and SMART. CDD is used to identify conserved domains within protein sequences, providing insights into their function. It is a key tool for protein sequence analysis. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">CDD</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000968 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000968">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000705"/>
        <obo:IAO_0000115 xml:lang="en">A database that provides a detailed and comprehensive hierarchical classification of protein structural domains based on their evolutionary relationships and structural similarities. SCOP2 builds upon the original SCOP, aiming to provide a more automated and comprehensive classification. It is a key resource for understanding protein structure and evolution. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">SCOP2</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000969 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000969">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000705"/>
        <obo:IAO_0000115 xml:lang="en">Class, Architecture, Topology, Homologous superfamily - A hierarchical classification database of protein domain structures derived from the Protein Data Bank (PDB). CATH groups protein domains based on their evolutionary relationships and structural similarities at different levels: Class (secondary structure content), Architecture (overall shape), Topology (fold family), and Homologous superfamily. It provides insights into protein structure, function, and evolution. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">CATH</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000970 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000970">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000705"/>
        <obo:IAO_0000115 xml:lang="en">Evolutionary Classification of Protein Domains - A hierarchical classification of protein domains based on evolutionary relationships, emphasizing distant homology. ECOD groups domains into families, homologous groups (H-groups), topological groups (T-groups), architectural groups (X-groups), and possible homology groups (Y-groups). It provides a framework for understanding protein evolution and structure. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">ECOD</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000971 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000971">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000706"/>
        <obo:IAO_0000115 xml:lang="en">A collection of manually drawn pathway maps representing current knowledge on molecular interaction and reaction networks for metabolism, genetic information processing, environmental information processing, cellular processes, organismal systems, and human diseases. KEGG Pathway is part of the larger Kyoto Encyclopedia of Genes and Genomes (KEGG) resource. It is widely used for pathway analysis and systems biology. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">KEGG Pathway</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000972 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000972">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000706"/>
        <obo:IAO_0000115 xml:lang="en">A free, open-source, curated, and peer-reviewed pathway database that provides detailed information on human biological pathways and processes. Reactome uses a hierarchical organization to describe molecular events, from individual reactions to complex pathways, linking genes, proteins, and small molecules. It is a key resource for systems biology and pathway analysis.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">Reactome</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000973 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000973">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000706"/>
        <obo:IAO_0000115 xml:lang="en">A collection of thousands of Pathway/Genome Databases (PGDBs), each describing the genome and metabolic pathways of a specific organism, plus a multi-organism pathway database (MetaCyc). BioCyc provides curated information on genes, enzymes, metabolites, and reactions, facilitating comparative genomics and metabolic network analysis. It is a key resource for systems biology. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">BioCyc</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000977 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000977">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000706"/>
        <obo:IAO_0000115 xml:lang="en">A community-curated, open, and collaborative platform for biological pathway diagrams. WikiPathways allows researchers to contribute, edit, and share pathway information, fostering a dynamic and up-to-date collection of pathway models across various species. It promotes open access to pathway knowledge. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">WikiPathways</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000991 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000991">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000707"/>
        <obo:IAO_0000115 xml:lang="en">A public, web-accessible database focusing on measured binding affinities of interactions between proteins (considered as drug targets) and small, drug-like molecules. It primarily contains data from enzyme assays, isothermal titration calorimetry, and surface plasmon resonance. BindingDB is a valuable resource for drug discovery, cheminformatics, and validating computational predictions. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">BindingDB</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000992 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000992">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000707"/>
        <obo:IAO_0000115 xml:lang="en">A public repository within PubChem that stores descriptions and results of biological screening assays on chemical substances, typically from high-throughput screening programs. PubChem BioAssay links chemical structures to their biological activities, providing valuable data for drug discovery and chemical biology. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">PubChem BioAssay</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000993 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000993">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000707"/>
        <obo:IAO_0000115 xml:lang="en">IUPHAR/BPS Guide to PHARMACOLOGY - An expert-curated database of pharmacological targets (receptors, ion channels, enzymes, transporters) and the drugs/ligands that act on them, developed by the International Union of Basic and Clinical Pharmacology (IUPHAR) and the British Pharmacological Society (BPS). It provides concise overviews, nomenclature, and key properties for these targets and ligands. It is an authoritative resource for pharmacology. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">IUPHAR/BPS</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000994 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000994">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000708"/>
        <obo:IAO_0000115 xml:lang="en">A discovery platform integrating and standardizing data on gene-disease associations (GDAs) and variant-disease associations (VDAs) from multiple public repositories and scientific literature. DisGeNET provides scores to rank these associations and tools for their analysis and interpretation. It is a valuable resource for studying the genetic basis of human diseases. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">DisGeNET</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000995 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000995">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000708"/>
        <obo:IAO_0000115 xml:lang="en">Online Mendelian Inheritance in Man - A comprehensive, authoritative, and continuously updated catalog of human genes and genetic disorders and traits, with a particular focus on the gene-phenotype relationship. OMIM provides textual information, references, and links to other genetic resources. It is an essential resource for human genetics and clinical research. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">OMIM</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000996 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000996">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000709"/>
        <obo:IAO_0000115 xml:lang="en">A general-purpose open-access repository operated by CERN and OpenAIRE, allowing researchers in all disciplines to deposit research outputs, including datasets, software, publications, and figures. Zenodo assigns DOIs to uploads, making them citable and discoverable. It supports FAIR data principles. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">Zenodo</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000997 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000997">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000709"/>
        <obo:IAO_0000115 xml:lang="en">A general-purpose, curated repository that makes the data underlying scientific and medical publications discoverable, freely reusable, and citable. Dryad provides a platform for researchers to archive and share diverse datasets associated with their research articles. It promotes open science and data reproducibility. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">Dryad Digital Repository</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000998 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000998">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000709"/>
        <obo:IAO_0000115 xml:lang="en">A general-purpose online digital repository where researchers can preserve and share their research outputs, including figures, datasets, images, and code. Figshare allows users to make their work citable and discoverable, supporting open science practices across various disciplines. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">Figshare</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_000999 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_000999">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000710"/>
        <rdfs:label xml:lang="en">MDDBR</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_001000 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_001000">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000710"/>
        <obo:IAO_0000115 xml:lang="en">A publicly accessible database designed to store, search, and analyze biomolecular simulation data, including trajectories, input parameters, and analysis results. It aims to promote reproducibility, data sharing, and re-use of simulation data within the molecular modeling community. BioSimDB facilitates the discovery and comparison of simulation studies. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">BioSimDB</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_001001 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_001001">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000710"/>
        <obo:IAO_0000115 xml:lang="en">A database and web server that provides a collection of pre-equilibrated molecular dynamics simulations of proteins, particularly focusing on pharmaceutically relevant targets. MoDEL aims to offer ready-to-use starting points for further simulation studies, such as ligand binding or conformational analysis. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">MoDEL</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_001002 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_001002">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000710"/>
        <obo:IAO_0000115 xml:lang="en">A web server and platform designed for sharing, visualizing, and analyzing molecular dynamics (MD) simulation trajectories directly in a web browser. MDsrv allows researchers to easily disseminate their MD data and provides interactive tools for common trajectory analyses without requiring local software installation. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">MDsrv</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_001003 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_001003">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000692"/>
        <obo:IAO_0000115 xml:lang="en">A prototype deposition and archiving system for structural models of biomolecular systems determined using integrative or hybrid (I/H) methods, which combine data from various experimental techniques and computational approaches. PDB-Dev aims to establish standards for archiving these complex structural models. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">PDB-Dev</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_001004 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_001004">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000711"/>
        <obo:IAO_0000115 xml:lang="en">A comprehensive repository providing open access to millions of high-accuracy protein structure predictions generated by DeepMind&apos;s AlphaFold AI system. It significantly expands structural coverage of known protein sequences, offering valuable models for proteins lacking experimental structures. This database is a key resource for structural biology and drug discovery. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">AlphaFold Protein Structure Database</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_001005 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_001005">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000711"/>
        <obo:IAO_0000115 xml:lang="en">A database of continuously updated, high-quality 3D protein structure models generated by the SWISS-MODEL homology modeling pipeline. It provides access to pre-computed models for a large number of protein sequences, particularly those from model organisms and UniProtKB. It is a key resource for obtaining theoretical protein models. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">SWISS-MODEL Repository</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_001006 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_001006">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000711"/>
        <obo:IAO_0000115 xml:lang="en">Evolutionary Scale Modeling Atlas - A database providing access to over 600 million metagenomic protein structure predictions generated using the ESMFold model, developed by Meta AI. ESMatlas significantly expands the structural coverage of proteins from diverse microbial environments. It is a valuable resource for exploring the structural landscape of the microbial world. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">ESMatlas</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_001007 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_001007">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000711"/>
        <obo:IAO_0000115 xml:lang="en">A public repository for archiving and distributing 3D structural models of biomacromolecules, particularly those generated by computational methods that are not eligible for deposition in the PDB. ModelArchive supports the FAIR (Findable, Accessible, Interoperable, Reusable) principles for theoretical models. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">ModelArchive</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_001025 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_001025">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000712"/>
        <obo:IAO_0000115 xml:lang="en">A database for cross-species, cross-technique metabolomics experiments and derived information, maintained at EMBL-EBI. MetaboLights archives metadata about studies, raw experimental data, and processed metabolite identifications and quantifications. It is a key repository for the metabolomics community. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">MetaboLights</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_001026 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_001026">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000712"/>
        <obo:IAO_0000115 xml:lang="en">Global Natural Products Social Molecular Networking - A web-based mass spectrometry ecosystem that enables the analysis, sharing, and discovery of natural products through molecular networking and community-curated spectral libraries. GNPS facilitates the identification of known and novel molecules from complex mixtures. It is a key platform for natural product research and metabolomics. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">GNPS</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_001027 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_001027">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000712"/>
        <obo:IAO_0000115 xml:lang="en">A public repository for metabolomics data and metadata, part of the NIH Common Fund&apos;s Metabolomics Program. It serves as a national and international data repository and coordinating center, providing tools for data analysis, visualization, and integration with other omics data. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">Metabolomics Workbench</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_001028 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_001028">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000713"/>
        <obo:IAO_0000115 xml:lang="en">A free search engine primarily accessing the MEDLINE database of references and abstracts on life sciences and biomedical topics, maintained by the U.S. National Library of Medicine (NLM) at the National Institutes of Health (NIH). PubMed is the foremost resource for searching biomedical literature. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">PubMed</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_001029 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_001029">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000713"/>
        <obo:IAO_0000115 xml:lang="en">A large abstract and citation database of peer-reviewed literature: scientific journals, books, and conference proceedings, owned by Elsevier. Scopus provides tools for tracking, analyzing, and visualizing research output across various disciplines. It is a major competitor to Web of Science for literature searching and citation analysis. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">Scopus</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_001030 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_001030">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000713"/>
        <obo:IAO_0000115 xml:lang="en">A multidisciplinary citation indexing service and database platform owned by Clarivate Analytics. Web of Science provides access to multiple databases covering scholarly journals, books, conference proceedings, and patents, offering comprehensive citation data and research analytics. It is a major tool for literature discovery and impact assessment. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">Web of Science</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://purl.obolibrary.org/obo/MOLSIM_001031 -->

    <owl:NamedIndividual rdf:about="http://purl.obolibrary.org/obo/MOLSIM_001031">
        <rdf:type rdf:resource="http://purl.obolibrary.org/obo/MOLSIM_000713"/>
        <obo:IAO_0000115 xml:lang="en">A freely accessible web search engine that indexes the full text or metadata of scholarly literature across an array of publishing formats and disciplines. While not a curated database, it serves as a primary discovery tool for finding academic papers, theses, books, and abstracts. It is widely used by researchers for literature searches. (UNVERIFIED)</obo:IAO_0000115>
        <rdfs:label xml:lang="en">Google Scholar</rdfs:label>
    </owl:NamedIndividual>
    


