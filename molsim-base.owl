     xml:base="http://purl.obolibrary.org/obo/molsim/molsim-base.owl"
     xmlns:dce="http://purl.org/dc/elements/1.1/"
     xmlns:obo="http://purl.obolibrary.org/obo/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:oboInOwl="http://www.geneontology.org/formats/oboInOwl#">
    <owl:Ontology rdf:about="http://purl.obolibrary.org/obo/molsim/molsim-base.owl">
        <owl:versionIRI rdf:resource="http://purl.obolibrary.org/obo/molsim/releases/2025-05-13/molsim-base.owl"/>
        <dce:type rdf:resource="http://purl.obolibrary.org/obo/IAO_8000001"/>
        <dcterms:description>Molsim is an interoperable domain ontology designed to semantically represent platform-agnostic molecular simulations as FAIR (Findable, Accessible, Interoperable, and Reusable) datasets. This ontology aims to standardize the representation of molecular simulation data, processes, and methodologies across different platforms and tools.</dcterms:description>
        <dcterms:license rdf:resource="https://creativecommons.org/licenses/by-nc-sa/4.0/"/>
        <dcterms:title>Molecular Simulation Ontology</dcterms:title>
        <rdfs:comment></rdfs:comment>
        <owl:versionInfo>2025-05-13</owl:versionInfo>
    </owl:Ontology>
    


