@prefix dce: <http://purl.org/dc/elements/1.1/> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix oboInOwl: <http://www.geneontology.org/formats/oboInOwl#> .
@base <http://purl.obolibrary.org/obo/molsim.owl#> .

<http://purl.obolibrary.org/obo/molsim.owl> rdf:type owl:Ontology ;
                                             owl:imports <http://purl.obolibrary.org/obo/molsim/components/molsim_units_component.owl> ,
                                                         <http://purl.obolibrary.org/obo/molsim/imports/chebi_import.owl> ,
                                                         <http://purl.obolibrary.org/obo/molsim/imports/ncit_import.owl> ,
                                                         <http://purl.obolibrary.org/obo/molsim/imports/uo_import.owl> ;
                                             <http://protege.stanford.edu/plugins/owl/protege#defaultLanguage> "en" ;
                                             dcterms:description "Molsim is an interoperable domain ontology designed to semantically represent platform-agnostic atomistic biomolecular simulations as FAIR (Findable, Accessible, Interoperable, and Reusable) datasets. This ontology aims to standardize the representation of atomistic biomolecular simulation data, processes, and methodologies across different platforms and tools." ;
                                             dcterms:license <https://creativecommons.org/licenses/by-nc-sa/4.0/> ;
                                             dcterms:title "Molecular Simulation Ontology" ;
                                             rdfs:comment "" .

