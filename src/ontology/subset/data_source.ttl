<http://purl.obolibrary.org/obo/molsim.owl> rdf:type owl:Ontology ;
                                             owl:imports <http://purl.obolibrary.org/obo/molsim/components/molsim_units_component.owl> ,
                                                         <http://purl.obolibrary.org/obo/molsim/imports/chebi_import.owl> ,
                                                         <http://purl.obolibrary.org/obo/molsim/imports/ncit_import.owl> ,
                                                         <http://purl.obolibrary.org/obo/molsim/imports/uo_import.owl> ;
                                             dcterms:description "Molsim is an interoperable domain ontology designed to semantically represent platform-agnostic molecular simulations as FAIR (Findable, Accessible, Interoperable, and Reusable) datasets. This ontology aims to standardize the representation of molecular simulation data, processes, and methodologies across different platforms and tools." ;
                                             dcterms:license <https://creativecommons.org/licenses/by-nc-sa/4.0/> ;
                                             dcterms:title "Molecular Simulation Ontology" ;
                                             rdfs:comment "" .

